{
  "entry": [
    "playground/src/main.tsx",
    "playground/vite.config.ts",
    "src/**/index.ts"
  ],
  "ignoreBinaries": ["plantuml"],
  "ignoreDependencies": ["@types/react", "@types/react-dom"],
  "project": ["*/**", "!docs/**"],
  "rules": {
    "exports": "off",
    "types": "off"
  },
  "vitest": {
    "entry": ["*/**/*.test.!(*.*)"]
  }
}
